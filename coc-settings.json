{
	"npm.binPath": "pnpm",
	"html-css-support.styleSheets": ["/home/zan/project/web/bootstrap-5.3.3-dist/css/bootstrap.min.css", "public/assets/css/bootstrap.min.css", "public/assets/css/style.css"],
	"html-css-support.enabledLanguages": ["html", "vue", "blade", "htmldjango", "typescriptreact", "javascriptreact", "jsx", "php"],
	"coc-class-css.enabledLanguages": ["html", "javascriptreact", "jsx"],
	"snippets.extends": {
		"php": ["php", "html"],
		"cpp": ["c"],
		"javascriptreact": ["javascript"],
		"typescript": ["javascript"]
	},
  "emmet.includeLanguages": {"blade": "html", "php": "html"},
  "emmet.syntaxProfiles": {"blade": "html"},
  "tsserver.maxTsServerMemory": 3072,
  "tailwindCSS.trace.server": "off",
	"prettier.useTabs": true,
	"prettier.tabWidth": 4,
	"prettier.disableLanguages": ["markdown"],
  "prettier.embeddedLanguageFormatting": "auto",
	"colors.enable": true,
	"prettier.printWidth": 920,
	"vimlsp.filetypes": ["vim", "lua"],
	"coc.preferences.formatOnSave": false,
	"workspace.ignoredFiletypes": ["NeogitStatus", "NeogitPopup", "NeogitCommitMessage"],
	"coc.source.file.ignorePatterns": ["NeogitStatus", "NeogitPopup", "NeogitCommitMessage"],
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
	"javascript.preferences.jsxAttributeCompletionStyle": "braces",
	"snippets.ultisnips.pythonPrompt": false,
	"markdownlint.config": {
		"MD013": false
	},
	"blade.bladeFormatter.enable": true,
  "blade.bladeFormatter.optIndentInnerHtml": true,
  "intelephense.format.enable": false,
}
